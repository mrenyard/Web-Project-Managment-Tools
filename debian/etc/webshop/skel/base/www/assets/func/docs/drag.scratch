<!doctype html>
<html class="no-js" lang="en">
  <head>
    <meta charset="utf-8">
    <title>[TITLE] - ramp.matt-laptop.lan</title>
    <meta name="description" content="[SUMMARY]">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <link rel="stylesheet" href="//dev.ramp.matt-laptop.lan/assets/style/import/base.css">
    <link rel="stylesheet" href="//dev.ramp.matt-laptop.lan/assets/style/scratch/drop.css">
    <script src="//dev.ramp.matt-laptop.lan/assets/func/extlibs/modernizr-custom.js"></script>
  </head>
  <body id="ramp-matt-laptop-lan">
    <dialog id="modal" open><form method="post">
      <header>
        <h2>Modal Dailog!</h2>
        <button formmethod="dialog">X</button>
      </header>
      <footer>
        <button formmethod="dialog">OK</button>
        <button hidden autofocus>Confirm</button>
      </footer>
    </form></dialog>
    <header role="banner">
      [LOGO] - [HEADER]
    </header>
    <main id="main">
      <header><a href="#header" title="Header full description">#</a>
        <h1>[HEADING]</h1>
        <p>[SUMMARY]</p>
      </header>
      <section class="ribbon">
        <article id="toolbar1" class="toolbar dropzone">
          <button id="button1" class="draggable">One</button>
          <button id="button2" class="draggable">Two</button>
          <button id="button3" class="draggable">Three</button>
          <button id="button4" class="draggable">Four</button>
          <button id="button5" class="draggable">Five</button>
        </article>
        <article id="toolbar2" class="toolbar dropzone">
          <button id="message" class="draggable">message</button>
          <button id="confirm" class="draggable">confirm</button>
          <button id="submit" class="draggable">submit</button>
          <button id="reload" class="draggable">reload</button>
        </article>
      </section>
      <section id="sort-one" class="sort"><a href="#sort-one" title="Zone two description">#</a>
        <section id="zone-one-one" class="dropzone">
          <article id="item1" class="draggable">
            <h4>Item One</h4>
            <p>Left</p>
          </article>
          <article id="item2" class="draggable">
            <h4>Item Two</h4>
            <p>Right</p>
          </article>
          <article id="item3" class="draggable">
            <h4>Item Three</h4>
            <p>Right</p>
          </article>
        </section>
        <section id="zone-one-two" class="dropzone">
          <article id="item4" class="draggable">
            <h4>Item Four</h4>
            <p>Left</p>
          </article>
          <article id="item5" class="draggable">
            <h4>Item Five</h4>
            <p>Left</p>
          </article>
          <article id="item6" class="draggable">
            <h4>Item Six</h4>
            <p>Left</p>
          </article>
          <article id="item7" class="draggable">
            <h4>Item Seven</h4>
            <p>Right</p>
          </article>
        </section>
      </section>
      <section id="sort-two" class="sort"><a href="#sort-two" title="Zone two description">#</a>
        <section id="zone-two-one" class="dropzone">
            <article id="article1" class="draggable">
              <h4>Article One</h4>
              <p>Left</p>
            </article>
            <article id="article2" class="draggable">
              <h4>Article Two</h4>
              <p>Right</p>
            </article>
            <article id="article3" class="draggable">
              <h4>Article Three</h4>
              <p>Centre</p>
            </article>
        </section>
        <section id="zone-two-two" class="dropzone">
          <article id="article4" class="draggable">
            <h4>Article Four</h4>
            <p>Right</p>
          </article>
          <article id="article5" class="draggable">
            <h4>Article Five</h4>
            <p>Left</p>
          </article>
        </section>
        <section id="zone-two-three" class="dropzone">
        </section>
      </section>
      <section class="diagram uml-class view-detail">
        <header><h2>Commonality and Variability Analysis (comparision)</h2></header>
        <div class="sheet">
          <article id="model" class="class abstract compact">
            <header contenteditable="true"><h3 title="Class: Model">Model</h3></header>
            <ol>
              <li class="properties"><h4>Properties</h4>
                <ul contenteditable="true">
                </ul>
              </li>
              <li class="methods"><h4>Methods</h4>
                <ul contenteditable="true">
                </ul>
              </li>
              <li class="associations"><h4>Associations</h4></li>
            </ol>
          </article>
        </div>
      </section>
    </main>
    <footer role="contentinfo">
      <p>[FOOTER]</p>
    </footer>
    <script src="/assets/func/init-full.js"></script>
    <script>
FUNC.modsPath = '/assets/func/mods/full/';
FUNC.init.register('sort', 'drag', ['event']);
FUNC.init.register('ribbon', 'toolbar', ['dom','event']);
FUNC.init.register('diagram', 'diagram');
FUNC.init.run();
    </script>
  </body>
</html>
